{
    "name": "razor/watermarker",
    "description": "An image watermark service module for Zend Framework 2",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "zf2", "thumbnail", "watermark", "image", "crop"
    ],
    "homepage": "https://github.com/kaiohken1982/Watermarker",
    "authors": [
        {
            "name": "sergio Rinaudo",
            "email": "sergio.rinaudo@gmail.com",
            "homepage": "http://www.sergiorinaudo.com/"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "ext-gd": "*",
        "razor/thumbnailer" : "1.*",
        "zendframework/zendframework": "2.*"
    },
    "require-dev": {
		"phpunit/phpunit" : "3.7.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "suggest": {
    },
    "autoload": {
        "psr-0": {
            "Watermarker": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    }
}